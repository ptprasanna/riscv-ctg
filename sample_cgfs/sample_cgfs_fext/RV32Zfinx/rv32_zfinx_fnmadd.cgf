fnmadd_b1:
    config: 
      - check ISA:=regex(.*I.*_Zfinx.*)
    opcode: 
      fnmadd.s: 0
    rs1: 
      <<: *all_regs
    rs2:
      <<: *all_regs
    rs3:
      <<: *all_regs
    rd: 
      <<: *all_regs
    op_comb: 
      <<: *r4fmt_op_comb
    val_comb:
      abstract_comb:
        'ibm_b1(32, "fnmadd.s", 3)': 0
        
