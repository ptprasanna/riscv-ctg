fmul_b1:
    config: 
      - check ISA:=regex(.*I.*_Zfinx.*)
    opcode: 
      fmul.s: 0
    rs1: 
      <<: *all_regs
    rs2:
      <<: *all_regs
    rd: 
      <<: *all_regs
    op_comb: 
      <<: *rfmt_op_comb
    val_comb:
      abstract_comb:
        'ibm_b1(32, "fmul.s", 2)': 0
        
