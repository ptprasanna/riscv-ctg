fcvt.s.l_b25:
    config: 
      - check ISA:=regex(.*I.*_Zfinx.*)
    opcode: 
      fcvt.s.l: 0
    rs1: 
      <<: *all_regs
    rd: 
      <<: *all_regs
    val_comb:
      abstract_comb:
        'ibm_b25(32, "fcvt.s.l", 1)': 0
        
fcvt.s.l_b26:
    config: 
      - check ISA:=regex(.*I.*_Zfinx.*)
    opcode: 
      fcvt.s.l: 0
    rs1: 
      <<: *all_regs
    rd: 
      <<: *all_regs
    val_comb:
      abstract_comb:
        'ibm_b26(32, "fcvt.s.l", 1)': 0

